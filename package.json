{
  "name": "immutable-react-react-redux-skeleton",
  "version": "1.0.0",
  "description": "Skeleton for react-redux with Immutabe.js, redux router and socket.io",
  "scripts": {
    "prestart": "babel-node tools/start-message.js",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/src-server.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "npm run styleguide-build && casperjs test tools/pixel-perfect-test-runner.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/build.html.js",
    "prebuild": "npm-run-all clean-dist lint test build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/dist-server.js",
    "styleguide-server": "styleguidist server",
    "styleguide-build": "styleguidist build"
  },
  "author": "MeetmeLeave",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "3.3.6",
    "jquery": "2.2.3",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-modal": "^1.6.1",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "redux": "3.5.2",
    "redux-immutable": "^3.0.8",
    "redux-saga": "^0.13.0",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-jest": "*",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "casperjs": "^1.1.2",
    "colors": "1.1.2",
    "compression": "1.6.1",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "jest": "^17.0.3",
    "jest-cli": "*",
    "jsdom": "8.5.0",
    "nock": "8.0.0",
    "npm-run-all": "1.8.0",
    "open": "0.0.5",
    "phantomjs-prebuilt": "^2.1.4",
    "react-addons-test-utils": "^15.3.0",
    "react-styleguidist": "^4.4.1",
    "react-test-renderer": "*",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "1.0.2",
    "resemblejs": "^2.2.2",
    "rimraf": "2.5.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MeetmeLeave/immutable-react-redux-skeleton"
  }
}