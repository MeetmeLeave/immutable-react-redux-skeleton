{
    "name": "immutable-react-react-redux-skeleton",
    "version": "1.0.0",
    "description": "Skeleton for react-redux with Immutabe.js, redux router and web sockets",
    "private": false,
    "scripts": {
        "dev": "npm-run-all --parallel open:src lint:watch",
        "open:src": "babel-node tools/src-server.js",
        "lint": "node_modules/.bin/esw webpack.config.* src tools",
        "lint:watch": "npm run lint -- --watch",
        "test": "npm run styleguide-build && casperjs test tools/pixel-perfect-test-runner.js",
        "clean-dist": "npm run remove-dist && mkdir dist",
        "remove-dist": "node_modules/.bin/rimraf ./dist",
        "build:html": "babel-node tools/build.html.js",
        "prebuild": "npm-run-all clean-dist lint test build:html",
        "build": "babel-node tools/build.js",
        "postbuild": "babel-node tools/dist-server.js",
        "styleguide-server": "styleguidist server",
        "styleguide-build": "styleguidist build"
    },
    "author": "MeetmeLeave",
    "license": "MIT",
    "dependencies": {
        "bootstrap": "^4.0.0-alpha.4",
        "immutable": "^3.8.1",
        "jquery": "3.2.1",
        "react": "^15.4.2",
        "react-dom": "^15.4.2",
        "react-modal": "^1.7.3",
        "react-redux": "^5.0.3",
        "react-router": "^2.8.1",
        "react-router-redux": "^4.0.8",
        "redux": "^3.6.0",
        "redux-immutable": "^4.0.0",
        "redux-saga": "^0.14.5",
        "ws": "^1.1.4"
    },
    "devDependencies": {
        "sass-loader": "^6.0.3",
        "url-loader": "^0.5.8",
        "webpack": "^2.3.3",
        "react-styleguidist": "^5.0.6",
        "react-test-renderer": "^15.4.2",
        "react-addons-test-utils": "^15.4.2",
        "css-loader": "^0.26.2",
        "postcss-loader": "^1.3.3",
        "extract-text-webpack-plugin": "^2.1.0",
        "jest-cli": "^19.0.2",
        "babel-jest": "^19.0.0",
        "bootstrap-loader": "^2.0.0",
        "bootstrap-sass": "^3.3.7",
        "babel-loader": "^6.4.1",
        "babel-plugin-transform-object-assign": "^6.22.0",
        "babel-polyfill": "^6.23.0",
        "babel-preset-es2015": "^6.24.0",
        "babel-preset-react": "^6.23.0",
        "babel-cli": "6.8.0",
        "babel-core": "6.8.0",
        "babel-plugin-react-display-name": "2.0.0",
        "babel-preset-react-hmre": "1.1.1",
        "babel-register": "6.8.0",
        "casperjs": "^1.1.2",
        "colors": "1.1.2",
        "compression": "1.6.1",
        "loaders": "^1.1.2",
        "cross-env": "1.0.7",
        "enzyme": "2.2.0",
        "eslint": "^3.19.0",
        "eslint-plugin-import": "^1.6.1",
        "eslint-plugin-react": "^5.0.1",
        "eslint-watch": "^3.0.1",
        "eventsource-polyfill": "0.9.6",
        "express": "4.13.4",
        "file-loader": "0.8.5",
        "jest": "^17.0.3",
        "jsdom": "8.5.0",
        "nock": "8.0.0",
        "node-sass": "^4.5.2",
        "npm-run-all": "1.8.0",
        "open": "0.0.5",
        "phantomjs-prebuilt": "^2.1.4",
        "redux-devtools": "^3.3.1",
        "redux-devtools-dock-monitor": "^1.1.1",
        "redux-devtools-log-monitor": "^1.1.1",
        "redux-immutable-state-invariant": "1.2.3",
        "redux-mock-store": "1.0.2",
        "resemblejs": "^2.2.2",
        "rimraf": "2.5.2",
        "style-loader": "0.13.1",
        "resolve-url-loader": "^2.0.2",
        "webpack-dev-middleware": "1.6.1",
        "webpack-hot-middleware": "2.10.0"
    },
    "jest": {
        "testEnvironment": "node"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MeetmeLeave/react-redux-ws-skeleton.git"
    }
}